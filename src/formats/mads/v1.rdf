              <rdfs:comment xml:lang="en" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">madsrdf:ComplexType is a resource whose label is the concatenation of labels from two or more Authority descriptions or two or more Variant descriptions or some combination of Authority and Variant descriptions, each of a madsrdf:SimpleType.</rdfs:comment>
              <rdfs:label xml:lang="en" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">Complex Type</rdfs:label>
              <rdfs:subClassOf rdf:resource="http://www.loc.gov/mads/rdf/v1#MADSType" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"/>
              <owl:disjointUnionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="http://www.loc.gov/mads/rdf/v1#HierarchicalGeographic"/>
                <rdf:Description rdf:about="http://www.loc.gov/mads/rdf/v1#ComplexSubject"/>
                <rdf:Description rdf:about="http://www.loc.gov/mads/rdf/v1#NameTitle"/>
              </owl:disjointUnionOf>
              <owl:disjointWith rdf:resource="http://www.loc.gov/mads/rdf/v1#SimpleType"/>
              <owl:equivalentClass>
                <owl:Restriction>
                  <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                  <owl:onProperty rdf:resource="http://www.loc.gov/mads/rdf/v1#componentList"/>
